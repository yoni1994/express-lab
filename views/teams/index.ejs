<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>First Express App</title>
</head>
<body>
    <h1>Favorite Teams</h1>
    <% if(error) { %>
      <h2><%= error %></h2>
    <% } %>
    <ul>
      <% teams.forEach(function(team) { %>
        <li>
          <%= team.name %>
            - Championships:
          <%= team.championships %>
        </li>
      <% }) %>
    </ul>
  </body>
</html>